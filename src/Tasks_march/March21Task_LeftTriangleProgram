package Tasks_march;

public class March21Task_LeftTriangleProgram {
    public static void main(String[] args) {

        int row =0;
        int col = 0;
        for( row=1; row<=5; row++){
            for(col= 1 ; col<=row; col++){
                System.out.print("*");

            }
            System.out.printf("%n");
        }
    }
}
